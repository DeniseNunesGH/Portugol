programa {
  funcao inicio() {
    //8 - Ler um valor N e imprimir todos os valores inteiros entre 1 (inclusive) e N (inclusive). Considere que o N será sempre maior que ZERO.
    //N  é um valor informado pelo usuário

    inteiro nN, v = 1

    escreva("Informe o valor: ")
    leia(nN)

    enquanto (v <= nN){
    escreva(v, "\n")
    v++
    //quando a quantidade definida for igual o programa irá parar, assim aumentando até parar
    }

  }
}
