programa {
  funcao inicio() {
    //Escreva um programa em que o usuário informe dois números. Então escreva em tela o maior deles

    inteiro variavel1
    inteiro variavel2

    escreva("1°: ")
    leia(variavel1)

    escreva("2°: ")
    leia(variavel2)

    se (variavel1 > variavel2){
      escreva("O 1° é o maior")

    } 
    senao {
      escreva("O 2° é o maior")
    }
  }
}
